{
    "name": "count023/php-slim-twig-frontend",
    "description": "PHP frontend for a Content-API for news using Slim and Twig, following the ADR-Pattern and should be run by a react server",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "slim3", "twig", "ADR", "react-server", "monolog"],
    "authors": [
        {
            "name": "Alexis Kremin",
            "email": "alexis.kremin@frenet.de",
            "homepage": "http://www.a-kremin.de/"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../php-json-marshaller"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "aura/payload": "^3.0",
        "anujrnair/php-json-marshaller": "*"
    }
}
